{
  "documents": [
    {
      "description": "Recent Git Changes",
      "outputPath": "docs/recent-changes.md",
      "sources": [
        {
          "type": "text",
          "description": "Documentation Header",
          "content": "# Recent Git Changes\n\nThis document contains recent changes from the git repository.\n",
          "tag": "description"
        },
        {
          "type": "git_diff",
          "description": "Recent Commits (Last 1)",
          "commit": "unstaged",
          "notPath": [
            "vendor",
            "tests"
          ],
          "showStats": true
        }
      ]
    },
    {
      "description": "Sources.",
      "outputPath": "api/sources.md",
      "sources": [
        {
          "type": "file",
          "sourcePaths": [
            "test"
          ],
          "filePattern": "*.php"
        },
        {
          "type": "file",
          "sourcePaths": [
            "src/Source",
            "src/Lib/Content"
          ],
          "filePattern": "*.php"
        }
      ]
    },
    {
      "description": "Modifiers",
      "outputPath": "api/modifiers.md",
      "sources": [
        {
          "type": "file",
          "sourcePaths": [
            "src/Modifier"
          ],
          "filePattern": "*.php"
        }
      ]
    },
    {
      "description": "Console commands",
      "outputPath": "api/console.md",
      "sources": [
        {
          "type": "file",
          "sourcePaths": [
            "src/Console"
          ],
          "filePattern": "*.php"
        }
      ]
    }
  ]
}